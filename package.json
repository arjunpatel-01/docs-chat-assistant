{
  "name": "docs-chat-assistant",
  "description": "NPM package to integrate an OpenAI Chat Assistant",
  "version": "0.0.1",
  "main": "./dist/docs-chat-assistant.js",
  "module": "./dist/docs-chat-assistant.cjs",
  "types": "./dist/docs-chat-assistant.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/docs-chat-assistant.js",
      "require": "./dist/docs-chat-assistant.cjs"
    },
    "./api": {
      "import": "./dist/api/docs-chat-assistant.js",
      "require": "./dist/api/docs-chat-assistant.cjs"
    }
