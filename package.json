  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "OpenAI",
    "assistant",
    "AI"
  ],
  "author": "arjunpatel-01",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arjunpatel-01/docs-chat-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/arjunpatel-01/docs-chat-assistant/issues"
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "cheerio": "^1.0.0-rc.12",
    "openai": "^4.47.2",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0"
  }
}
